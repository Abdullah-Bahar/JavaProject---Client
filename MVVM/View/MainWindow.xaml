<Window x:Class="JavaProject___Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JavaProject___Client"
        xmlns:viewmodel="clr-namespace:JavaProject___Client.MVVM.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <viewmodel:MainViewModel/>
    </Window.DataContext>
    
    <Grid>
        <Button x:Name="ConnectToServerButton" Content="Sunucuya bağlan"
                Command="{Binding ConnectToServer}" Click="ConnectServer_Button_Click">
        </Button>
    </Grid>
</Window>
