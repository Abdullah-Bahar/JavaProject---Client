<UserControl x:Class="JavaProject___Client.MVVM.View.HomeViewTweet"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:JavaProject___Client.MVVM.View"
             mc:Ignorable="d" 
             Background="#2F3136"
             d:DesignHeight="650" d:DesignWidth="1200">
    <Grid>
        <Label Content="{Binding Username}" FontSize="25"/>
        <Label Content="{Binding UID}" FontSize="25" Margin="0,50,0,0"/>
        <Button 
            Content="Kullanıcalara git" 
            Command="{Binding NavigateToHomeUser}"
            Width="150"
            Height="30"/>
            
    </Grid>
</UserControl>
